  crglangmgr.dll finmgrapp.dll langeditor.dll cif_cus.dll dlgbox.dll fin_app.exe \
  cif_acc.dll StopWatchApp.exe FilelistCreater.exe Insignia.dll InsigniaReg.dll \
  donations.dll Favor.dll mttObjective.dll FavorReg.dll EndPeroid.dll \
  runmydll32.exe projects.dll backoffice.dll FndUtils.dll reports.dll RGLib.dll \
  dnoinquiry.dll Endorsee.dll EndMonth.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
